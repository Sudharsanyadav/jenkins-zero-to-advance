pipeline{
    agent any
    stages{
        stage("build"){
            steps{
                 echo 'this is a appication building..'
            }
        }
        stage("test stage"){
            steps{
               echo 'this i a application testing'
            }
        }
        stage("deploy stage"){
            steps{
            echo "this is a application deploying "
            }
        }
    }
}
